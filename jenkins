pipeline
  {
   agent any
   stages
   {
   stage("checkout source code")
   {
     steps
     {
      git branch: 'franch3_dev3', url: 'git@github.com:utpaldey108/multibrunch_example.git'
     }
    }
   stage("checkforakeyword")
   {
    steps
    {
     sh 'cat contact.html | grep -i wealth'
    }
   }
  }
}
