pipeline
  {
   agent any
   stages
   {
   stage("checkout source code")
   {
     steps
     {
      git branch: 'feature3', url: 'https://github.com/utpaldey108/multibrunch_example.git'
     }
    }
   stage("checkforakeyword")
   {
    steps
    {
     sh 'cat contact.html | grep -i Hospital'
    }
   }
  }
}
